{
	"compact": false,
	"presets": [
		["@babel/preset-env", {
			"targets": {
				"browsers": ["last 2 Chrome versions"]
			},
			"loose": true,
			"useBuiltIns": "usage",
			"modules": false,
			"debug": true,
			"spec": false
		}],
		"@babel/preset-react",
		"@babel/preset-typescript",
		"@babel/preset-stage-2"
	],
	"plugins": [
		"transform-decorators-legacy",
		"transform-class-properties",
		"transform-object-rest-spread"
	],
	"env": {
		"test": {
			"presets": [
				["@babel/preset-env", {
					"targets": {
						"esmodules": true
					},
					"loose": true,
					"modules": false,
					"debug": true,
					"spec": false
				}],
				"@babel/preset-react",
				"@babel/preset-typescript"
			]
		}
	}
}
